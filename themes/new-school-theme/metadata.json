            },
            "title": {
              "type": "short-text",
              "label": "Section Title"
            }
          }
        },
        "sections": {
          "type": "object",
          "label": "Content Sections",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Section Title"
            },
            "content": {
              "type": "long-text",
              "label": "Section Content"
            },
            "intro": {
              "type": "long-text",
              "label": "Section Introduction"
