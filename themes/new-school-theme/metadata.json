            },
            "phone": {
              "type": "short-text",
              "label": "Contact Phone"
            },
            "address": {
              "type": "short-text",
              "label": "Contact Address"
            }
          }
        }
      }
    }
  }
},
    "privacy-policy": {
      "type": "page",
      "title": "Privacy Policy",
      "sections": {
        "main": {
          "title": "Privacy Content",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Page Title",
              "minLength": 1
            },
            "last_updated": {
              "type": "short-text",
              "label": "Last Updated Date",
              "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
            },
            "sections": {
              "type": "object",
              "label": "Privacy Policy Sections",
              "fields": {
                "information_collected": {
                  "type": "object",
                  "label": "Information We Collect",
                  "fields": {
                    "title": {
                      "type": "short-text",
                      "label": "Section Title"
                    },
                    "personal_info": {
                      "type": "object",
                      "fields": {
                        "title": {
                          "type": "short-text",
                          "label": "Personal Information Title"
                        },
                        "items": {
                          "type": "array",
                          "label": "Personal Information Items",
                          "items": {
                            "type": "long-text"
                          }
                        }
                      }
                    },
                    "technical_info": {
                      "type": "object",
                      "fields": {
                        "title": {
                          "type": "short-text",
                          "label": "Technical Information Title"
                        },
                        "items": {
                          "type": "array",
                          "label": "Technical Information Items",
                          "items": {
                            "type": "long-text"
                          }
                        }
                      }
                    }
                  }
                },
                "information_usage": {
                  "type": "object",
                  "label": "How We Use Information",
                  "fields": {
                    "title": {
                      "type": "short-text",
                      "label": "Section Title"
                    },
                    "items": {
                      "type": "array",
                      "label": "Usage Items",
                      "items": {
                        "type": "long-text"
                      }
                    }
                  }
                },
                "data_protection": {
                  "type": "object",
                  "label": "Data Protection",
                  "fields": {
                    "title": {
                      "type": "short-text",
                      "label": "Section Title"
                    },
                    "items": {
                      "type": "array",
                      "label": "Protection Measures",
                      "items": {
                        "type": "long-text"
                      }
                    }
                  }
                },
                "information_sharing": {
                  "type": "object",
                  "label": "Information Sharing",
                  "fields": {
