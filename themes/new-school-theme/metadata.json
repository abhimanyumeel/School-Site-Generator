{
  "theme": "new-school-theme",
  "displayName": "New School Theme",
  "description": "A modern and clean Hugo theme for school websites",
  "version": "1.0.0",
  "pages": {
    "global": {
      "type": "page",
      "title": "Global Settings",
      "sections": {
        "site_name": {
          "title": "Site Name",
          "fields": {
            "name": {
              "type": "short-text",
              "label": "School Name",
              "required": true
            }
          }
        },
        "site_logo": {
          "title": "Site Logo",
          "fields": {
            "icon": {
              "type": "image",
              "label": "Logo Icon",
              "required": true,
              "description": "Font Awesome icon class"
            },
            "text": {
              "type": "short-text",
              "label": "Logo Text",
              "required": true,
              "description": "Logo text"
            }
          }
        },
        "navigation": {
          "title": "Navigation",
          "fields": {
            "main_menu": {
              "type": "array",
              "label": "Main Menu Items",
              "items": {
                "name": {
                  "type": "short-text",
                  "label": "Menu Item Name"
                },
                "url": {
                  "type": "short-text",
                  "label": "Menu Item URL"
                },
                "type": {
                  "type": "select",
                  "label": "Menu Type",
                  "options": ["link", "dropdown"]
                },
                "items": {
                  "type": "array",
                  "items": {
                    "name": {
                      "type": "short-text",
                      "label": "Submenu Item Name"
                    },
                    "url": {
                      "type": "short-text",
                      "label": "Submenu Item URL"
                    }
                  }
                }
              }
            }
          }
        },
        "footer": {
          "title": "Footer",
          "fields": {
            "about": {
              "type": "object",
              "label": "About Section",
              "fields": {
                "description": {
                  "type": "long-text",
                  "label": "About Description"
                },
                "social_links": {
                  "type": "object",
                  "label": "Social Links",
                  "fields": {
                    "facebook": {
                      "type": "short-text",
                      "label": "Facebook URL"
                    },
                    "twitter": {
                      "type": "short-text",
                      "label": "Twitter URL"
                    },
                    "linkedin": {
                      "type": "short-text",
                      "label": "LinkedIn URL"
                    },
                    "youtube": {
                      "type": "short-text",
                      "label": "YouTube URL"
                    }
                  }
                }
              }
            },
            "quick_links": {
              "type": "array",
              "label": "Quick Links",
              "items": {
                "name": {
                  "type": "short-text",
                  "label": "Link Name"
                },
                "url": {
                  "type": "short-text",
                  "label": "Link URL"
                }
              }
            }
          }
        }
      }
    },
    "home": {
      "type": "page",
      "title": "Home Page",
      "sections": {
        "hero": {
          "title": "Hero Section",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Hero Title",
              "required": true
            },
            "subtitle": {
              "type": "short-text",
              "label": "Hero Subtitle",
              "required": true
            },
            "description": {
              "type": "long-text",
              "label": "Hero Description",
              "required": true
            },
            "cta": {
              "type": "object",
              "label": "Call to Action",
              "fields": {
                "primary": {
                  "type": "object",
                  "fields": {
                    "text": {
                      "type": "short-text",
                      "label": "Primary Button Text"
                    },
                    "url": {
                      "type": "short-text",
                      "label": "Primary Button URL"
                    }
                  }
                },
                "secondary": {
                  "type": "object",
                  "fields": {
                    "text": {
                      "type": "short-text",
                      "label": "Secondary Button Text"
                    },
                    "url": {
                      "type": "short-text",
                      "label": "Secondary Button URL"
                    }
                  }
                }
              }
            }
          }
        },
        "featured_classes": {
          "title": "Featured Classes",
          "fields": {
            "items": {
              "type": "array",
              "label": "Class Items",
              "items": {
                "name": {
                  "type": "short-text",
                  "label": "Class Name"
                },
                "age_group": {
                  "type": "short-text",
                  "label": "Age Group"
                },
                "time": {
                  "type": "short-text",
                  "label": "Class Time"
                },
                "capacity": {
                  "type": "short-text",
                  "label": "Class Capacity"
                },
                "price": {
                  "type": "short-text",
                  "label": "Class Price"
                },
                "image": {
                  "type": "image",
                  "label": "Class Image"
                },
                "teacher": {
                  "type": "object",
                  "label": "Teacher Information",
                  "fields": {
                    "name": {
                      "type": "short-text",
                      "label": "Teacher Name"
                    },
                    "image": {
                      "type": "image",
                      "label": "Teacher Image"
                    },
                    "position": {
                      "type": "short-text",
                      "label": "Teacher Position"
                    }
                  }
                }
              }
            }
          }
        },
        "events": {
          "title": "Events Section",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Events Section Title",
              "required": true
            },
            "description": {
              "type": "long-text",
              "label": "Events Section Description",
              "required": true
            },
            "items": {
              "type": "array",
              "label": "Event Items",
              "items": {
                "title": {
                  "type": "short-text",
                  "label": "Event Title",
                  "required": true
                },
                "description": {
                  "type": "long-text",
                  "label": "Event Description",
                  "required": true
                },
                "date": {
                  "type": "short-text",
                  "label": "Event Date",
                  "required": true
                },
                "time": {
                  "type": "short-text",
                  "label": "Event Time",
                  "required": true
                },
                "location": {
                  "type": "short-text",
                  "label": "Event Location",
                  "required": true
                },
                "image": {
                  "type": "image",
                  "label": "Event Image",
                  "required": true
                }
              }
            }
          }
        }
      }
    },
    "about": {
      "type": "page",
      "title": "About Page",
      "sections": {
        "main": {
          "title": "Main Content",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Page Title",
              "required": true
            },
            "description": {
              "type": "long-text",
              "label": "Page Description",
              "minLength": 5,
              "description": "Use line breaks to separate paragraphs"
            },
            "mission": {
              "type": "long-text",
              "label": "Mission Statement",
              "minLength": 5
            },
            "vision": {
              "type": "long-text",
              "label": "Vision Statement",
              "minLength": 5
            },
            "ceo": {
              "type": "object",
              "label": "CEO Information",
              "fields": {
                "name": {
                  "type": "short-text",
                  "label": "CEO Name",
                  "required": true
                },
                "position": {
                  "type": "short-text",
                  "label": "CEO Position",
                  "required": true
                },
                "image": {
                  "type": "image",
                  "label": "CEO Image"
                }
              }
            }
          }
        }
      }
    },
"contact": {
  "type": "page",
  "title": "Contact Page",
  "sections": {
    "header": {
      "title": "Page Header",
      "fields": {
        "title": {
          "type": "short-text",
          "label": "Page Title",
          "required": true
        },
        "description": {
          "type": "long-text",
          "label": "Page Description",
          "required": true
        }
      }
    },
    "contact_info": {
      "title": "Contact Information",
      "fields": {
        "address": {
          "type": "object",
          "label": "Address Details",
          "fields": {
            "icon": {
              "type": "short-text",
              "label": "Address Icon",
              "required": true
            },
            "text": {
              "type": "short-text",
              "label": "Address Text",
              "required": true
            }
          }
        },
        "email": {
          "type": "object",
          "label": "Email Details",
          "fields": {
            "icon": {
              "type": "short-text",
              "label": "Email Icon",
              "required": true
            },
            "text": {
              "type": "short-text",
              "label": "Email Address",
              "required": true
            }
          }
        },
        "phone": {
          "type": "object",
          "label": "Phone Details",
          "fields": {
            "icon": {
              "type": "short-text",
              "label": "Phone Icon",
              "required": true
            },
            "text": {
              "type": "short-text",
              "label": "Phone Number",
              "required": true
            }
          }
        }
      }
    },
    "contact_form": {
      "title": "Contact Form",
      "fields": {
        "note": {
          "type": "long-text",
          "label": "Form Note"
        },
        "submit_text": {
          "type": "short-text",
          "label": "Submit Button Text",
          "required": true
        },
        "form_fields": {
          "type": "array",
          "label": "Form Fields",
          "items": {
            "name": {
              "type": "short-text",
              "label": "Field Name",
              "required": true
            },
            "type": {
              "type": "select",
              "label": "Field Type",
              "options": ["text", "email", "textarea"],
              "required": true
            },
            "label": {
              "type": "short-text",
              "label": "Field Label",
              "required": true
            },
            "placeholder": {
              "type": "short-text",
              "label": "Field Placeholder",
              "required": true
            }
          }
        }
      }
    },
    "map": {
      "title": "Map Settings",
      "fields": {
        "url": {
          "type": "short-text",
          "label": "Google Maps URL",
          "required": true
        }
      }
    },
    "business_hours": {
      "title": "Business Hours",
      "fields": {
        "weekdays": {
          "type": "object",
          "label": "Weekday Hours",
          "fields": {
            "days": {
              "type": "short-text",
              "label": "Days",
              "required": true
            },
            "hours": {
              "type": "short-text",
              "label": "Hours",
              "required": true
            }
          }
        },
        "weekend": {
          "type": "object",
          "label": "Weekend Hours",
          "fields": {
            "days": {
              "type": "short-text",
              "label": "Days",
              "required": true
            },
            "hours": {
              "type": "short-text",
              "label": "Hours",
              "required": true
            }
          }
        }
      }
    }
  }
},
"admission": {
  "type": "page",
  "title": "Admission Page",
  "sections": {
    "welcome": {
      "title": "Welcome Section",
      "fields": {
        "title": {
          "type": "short-text",
          "label": "Welcome Title"
        },
        "description": {
          "type": "long-text",
          "label": "Welcome Description",
          "description": "Use line breaks (\\n\\n) to separate paragraphs"
        }
      }
    },
    "contact_info": {
      "title": "Contact Information",
      "fields": {
        "title": {
          "type": "short-text",
          "label": "Section Title",
          "required": true
        },
        "contacts": {
          "type": "array",
          "label": "Contact Persons",
          "items": {
            "info": {
              "type": "object",
              "fields": {
                "name": {
                  "type": "short-text",
                  "label": "Contact Name"
              },
              "phone": {
                "type": "short-text",
                "label": "Phone Number"
              },
              "role": {
                "type": "short-text",
                "label": "Role/Position"
              }
            }
          }
        }
      },
        "campuses": {
          "type": "array",
          "label": "Campus Information",
          "items": {
            "info": {
            "type": "object",
            "fields": {
              "name": {
                "type": "short-text",
                "label": "Campus Name",
                "required": true
              },
              "type": {
                "type": "short-text",
                "label": "Enquiry Type",
                "required": true
              }
            }
          }
        }
      }
    }
  },
    "form": {
      "title": "Admission Form",
      "fields": {
        "title": {
          "type": "short-text",
          "label": "Form Title",
          "required": true
        },
        "fields": {
          "type": "array",
          "label": "Form Fields",
          "items": {
            "connect": {
            "type": "object",
            "fields": {
              "type": {
                "type": "select",
                "label": "Field Type",
                "options": ["text", "tel", "email"]
              },
              "name": {
                "type": "short-text",
                "label": "Field Name"
              },
              "placeholder": {
                "type": "short-text",
                "label": "Field Placeholder"
              },
              "required": {
                "type": "boolean",
                "label": "Is Required",
                "default": true
              }
            }
          }
        }
      },
        "class_options": {
          "type": "array",
          "label": "Available Classes",
          "items": {
            "class": {
              "type": "short-text",
              "label": "Class Name"
            }
          }
        }
      }
    }
  }
},
    "gallery": {
      "type": "page",
      "title": "Gallery Page",
      "sections": {
        "main": {
          "title": "Gallery",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Page Title",
              "minLength": 1
            },
            "description": {
              "type": "long-text",
              "label": "Gallery Description",
              "minLength": 1
            },
            "filters": {
              "type": "object",
              "label": "Gallery Filters",
              "fields": {
                "categories": {
                  "type": "array",
                  "label": "Category Filters",
                  "items": {
                    "label": {
                      "type": "short-text",
                      "label": "Category Label"
                    },
                    "value": {
                      "type": "short-text",
                      "label": "Category Value"
                    }
                  }
                },
                "years": {
                  "type": "array",
                  "label": "Year Filters",
                  "items": {
                    "label": {
                      "type": "short-text",
                      "label": "Year Label"
                    },
                    "value": {
                      "type": "short-text",
                      "label": "Year Value"
                    }
                  }
                }
              }
            },
            "items": {
              "type": "array",
              "label": "Gallery Items",
              "items": {
                "title": {
                  "type": "short-text",
                  "label": "Image Title",
                  "minLength": 1
                },
                "description": {
                  "type": "long-text",
                  "label": "Image Description",
                  "minLength": 1
                },
                "category": {
                  "type": "short-text",
                  "label": "Image Category",
                  "minLength": 1
                },
                "year": {
                  "type": "short-text",
                  "label": "Image Year",
                  "minLength": 1
                },
                "image": {
                  "type": "image",
                  "label": "Gallery Image",
                  "pattern": "^/images/.*\\.(jpg|jpeg|png|webp)$"
                }
              }
            }
          }
        }
      }
    },
    "facilities": {
      "type": "page",
      "title": "Facility Page",
      "sections": {
        "main": {
          "title": "Facilities",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Page Title"
            },
            "description": {
              "type": "long-text",
              "label": "Page Description"
            },
            "items": {
              "type": "array",
              "label": "Facility Items",
              "items": {
                "name": {
                  "type": "short-text",
                  "label": "Facility Name"
                },
                "description": {
                  "type": "long-text",
                  "label": "Facility Description"
                },
                "icon": {
                  "type": "short-text",
                  "label": "Facility Icon",
                  "description": "Font Awesome icon class"
                }
              }
            }
          }
        }
      }
    },
    "team": {
      "type": "page",
      "title": "Team Page",
      "sections": {
        "main": {
          "title": "Team Members",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Page Title",
              "minLength": 1
            },
            "members": {
              "type": "array",
              "label": "Team Members",
              "items": {
                "name": {
                  "type": "short-text",
                  "label": "Member Name",
                  "minLength": 1
                },
                "position": {
                  "type": "short-text",
                  "label": "Position",
                  "minLength": 1
                },
                "department": {
                  "type": "short-text",
                  "label": "Department",
                  "minLength": 1
                },
                "college": {
                  "type": "short-text",
                  "label": "College Name",
                  "minLength": 1
                },
                "bio": {
                  "type": "long-text",
                  "label": "Biography",
                  "minLength": 10,
                  "required": false
                },
                "image": {
                  "type": "image",
                  "label": "Member Photo",
                  "pattern": "^/images/.*\\.(jpg|jpeg|png|webp)$"
                }
              }
            }
          }
        }
      }
    },
    "blog": {
      "type": "page",
      "title": "Blog Page",
      "sections": {
        "main": {
          "title": "Blog Posts",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Page Title",
              "minLength": 1
            },
            "posts": {
              "type": "array",
              "label": "Blog Posts",
              "items": {
                "title": {
                  "type": "short-text",
                  "label": "Post Title",
                  "minLength": 1
                },
                "content": {
                  "type": "long-text",
                  "label": "Post Content",
                  "minLength": 10
                },
                "image": {
                  "type": "image",
                  "label": "Featured Image",
                  "pattern": "^/images/.*\\.(jpg|jpeg|png|webp)$"
                },
                "date": {
                  "type": "short-text",
                  "label": "Publication Date",
                  "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
                }
              }
            }
          }
        }
      }
    },
    
"terms-of-service": {
  "type": "page",
  "title": "Terms of Service",
  "sections": {
    "main": {
      "title": "Terms Content",
      "fields": {
        "title": {
          "type": "short-text",
          "label": "Page Title"
        },
        "last_updated": {
          "type": "short-text",
          "label": "Last Updated Date"
        },
        "navigation": {
          "type": "array",
