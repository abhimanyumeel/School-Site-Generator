{
  "theme": "school-temp",
  "displayName": "school-template",
  "description": "A modern and clean Hugo theme for school websites",
  "version": "1.0.0",
  "pages": {
    "home": {
      "type": "page",
      "title": "Home Page",
      "sections": {
        "hero": {
          "title": "Hero Section",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Hero Title",
              "required": true
            },
            "subtitle": {
              "type": "short-text",
              "label": "Hero Subtitle",
              "required": true
            },
            "description": {
              "type": "long-text",
              "label": "Hero Description",
              "required": true
            },
            "image": {
              "type": "image",
              "label": "Hero Background Image",
              "required": true,
              "description": "Large background image for hero section (recommended: 1920x1080px)"
            },
            "cta": {
              "type": "object",
              "label": "Call to Action",
              "fields": {
                "primary": {
                  "type": "object",
                  "fields": {
                    "text": {
                      "type": "short-text",
                      "label": "Primary Button Text"
                    },
                    "url": {
                      "type": "short-text",
                      "label": "Primary Button URL"
                    }
                  }
                },
                "secondary": {
                  "type": "object",
                  "fields": {
                    "text": {
                      "type": "short-text",
                      "label": "Secondary Button Text"
                    },
                    "url": {
                      "type": "short-text",
                      "label": "Secondary Button URL"
                    }
                  }
                }
              }
            }
          }
        },
        "notices": {
          "title": "Notice Board Section",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Notice Board Title",
              "required": true
            },
            "items": {
              "type": "array",
              "label": "Notice Items",
              "items": {
                "title": {
                  "type": "short-text",
                  "label": "Notice Title",
                  "required": true
                },
                "link": {
                  "type": "short-text",
                  "label": "Notice Link",
                  "required": true
                }
              }
            }
          }
        },
        "events": {
          "title": "Events Section",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Events Section Title",
              "required": true
            },
            "description": {
              "type": "long-text",
              "label": "Events Section Description",
              "required": true
            },
            "items": {
              "type": "array",
              "label": "Event Items",
              "items": {
                "title": {
                  "type": "short-text",
                  "label": "Event Title",
                  "required": true
                },
                "description": {
                  "type": "long-text",
                  "label": "Event Description",
                  "required": true
                },
                "date": {
                  "type": "short-text",
                  "label": "Event Date",
                  "required": true,
                  "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
                },
                "time": {
                  "type": "short-text",
                  "label": "Event Time",
                  "required": true
                },
                "location": {
                  "type": "short-text",
                  "label": "Event Location",
                  "required": true
                },
                "image": {
                  "type": "image",
                  "label": "Event Image",
                  "required": true
                }
              }
            }
          }
        }
      }
    },
    "admission": {
      "type": "page",
      "title": "Admission Page",
      "sections": {
        "welcome": {
          "title": "Welcome Section",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Welcome Section Title",
              "minLength": 1,
              "required": true
            },
            "description": {
              "type": "long-text",
              "label": "Welcome Description Paragraphs"
            }
          }
        },
        "contact_info": {
          "title": "Contact Information",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Contact Section Title",
              "required": true,
              "default": "Contact Information"
            },
            "contacts": {
              "type": "array",
              "label": "Contact Persons",
              "default": [
                {
                  "name": "Admission Coordinator",
                  "phone": "+1 (555) 123-4567"
                }
              ],
              "items": {
                "type": "object",
                "fields": {
                  "name": {
                    "type": "short-text",
                    "label": "Contact Name",
                    "required": true
                  },
                  "phone": {
                    "type": "short-text",
                    "label": "Phone Number",
                    "required": true
                  }
                }
              }
            }
          }
        },
        "form": {
          "title": "Admission Form",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Form Title",
              "required": true
            },
            "fields": {
              "type": "array",
              "label": "Form Fields",
              "items": {
                "type": "object",
                "fields": {
                  "type": {
                    "type": "select",
                    "label": "Field Type",
                    "options": ["text", "email", "tel", "select"],
                    "required": true
                  },
                  "placeholder": {
                    "type": "short-text",
                    "label": "Placeholder Text",
                    "required": true
                  },
                  "required": {
                    "type": "boolean",
                    "label": "Is Required",
                    "default": false
                  }
                }
              }
            },
            "class_options": {
              "type": "array",
              "label": "Available Class Options",
              "items": {
                "type": "short-text",
                "label": "Class Name",
                "required": true
              }
            },
            "terms_checkbox": {
              "type": "object",
              "label": "Terms Checkbox",
              "fields": {
                "label": {
                  "type": "short-text",
                  "label": "Checkbox Label",
                  "default": "I acknowledge that I have read, and do hereby accept the terms and conditions.",
                  "required": true
                }
              }
            },
            "submit_button": {
              "type": "object",
              "label": "Submit Button",
              "fields": {
                "text": {
                  "type": "short-text",
                  "label": "Button Text",
                  "default": "Submit",
                  "required": true
                }
              }
            }
          }
        }
      }
    },
    "about": {
      "type": "page",
      "title": "About Page",
      "sections": {
        "main": {
          "title": "Main Content",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Page Title",
              "required": true
            },
            "description": {
              "type": "array",
              "label": "Page Description",
              "items": {
                "type": "long-text",
                "minLength": 5
              }
            },
            "mission": {
              "type": "long-text",
              "label": "Mission Statement",
              "minLength": 5
            },
            "vision": {
              "type": "long-text",
              "label": "Vision Statement",
              "minLength": 5
            },
            "ceo": {
              "type": "object",
              "label": "CEO Information",
              "fields": {
                "name": {
                  "type": "short-text",
                  "label": "CEO Name",
                  "required": true
                },
                "position": {
                  "type": "short-text",
                  "label": "CEO Position",
                  "required": true
                },
                "image": {
                  "type": "image",
                  "label": "CEO Image"
                }
              }
            }
          }
        },
        "team": {
          "title": "Team Section",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Section Title",
              "required": true,
              "default": "Our Faculty"
            },
            "description": {
              "type": "long-text",
              "label": "Section Description",
              "required": true,
              "default": "Meet our experienced and dedicated faculty members"
            },
            "members": {
              "type": "array",
              "label": "Team Members",
              "items": {
                "type": "object",
                "fields": {
                  "name": {
                    "type": "short-text",
                    "label": "Member Name",
                    "required": true
                  },
                  "position": {
                    "type": "short-text",
                    "label": "Position",
                    "required": true
                  },
                  "department": {
                    "type": "short-text",
                    "label": "Department",
                    "required": true
                  },
                  "college": {
                    "type": "short-text",
                    "label": "College",
                    "required": true
                  },
                  "image": {
                    "type": "image",
                    "label": "Member Photo",
                    "required": true
                  },
                  "bio": {
                    "type": "long-text",
                    "label": "Biography",
                    "required": false
                  }
                }
              }
            }
          }
        }
      }
    },
    "contact": {
      "type": "page",
      "title": "Contact Page",
      "sections": {
        "main": {
          "title": "Contact Information",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Page Title",
              "minLength": 1,
              "default": "Contact Us",
              "required": true
            },
            "description": {
              "type": "long-text",
              "label": "Contact Description",
              "minLength": 5,
              "default": "Get in touch with us for any questions or inquiries",
              "required": true
            },
            "info": {
              "type": "object",
              "label": "Contact Information",
              "fields": {
                "address": {
                  "type": "object",
                  "fields": {
                    "icon": {
                      "type": "short-text",
                      "label": "Address Icon Class",
                      "default": "fas fa-map-marker-alt",
                      "required": true
                    },
                    "text": {
                      "type": "long-text",
                      "label": "Address Text",
                      "required": true
                    }
                  }
                },
                "email": {
                  "type": "object",
                  "fields": {
                    "icon": {
                      "type": "short-text",
                      "label": "Email Icon Class",
                      "default": "fas fa-envelope",
                      "required": true
                    },
                    "text": {
                      "type": "short-text",
                      "label": "Email Address",
                      "format": "email",
                      "required": true
                    }
                  }
                },
                "phone": {
                  "type": "object",
                  "fields": {
                    "icon": {
                      "type": "short-text",
                      "label": "Phone Icon Class",
                      "default": "fas fa-phone",
                      "required": true
                    },
                    "text": {
                      "type": "short-text",
                      "label": "Phone Number",
                      "required": true
                    }
                  }
                }
              }
            },
            "form": {
              "type": "object",
              "label": "Contact Form",
              "fields": {
                "note": {
                  "type": "long-text",
                  "label": "Form Note",
                  "default": "Please fill out the form below to get in touch with us.",
                  "required": true
                },
                "submit_text": {
                  "type": "short-text",
                  "label": "Submit Button Text",
                  "default": "Send Message",
                  "required": true
                },
                "fields": {
                  "type": "array",
                  "label": "Form Fields",
                  "default": [
                    {
                      "name": "name",
                      "label": "Your Name",
                      "type": "text",
                      "placeholder": "Enter your name"
                    },
                    {
                      "name": "email",
                      "label": "Your Email",
                      "type": "email",
                      "placeholder": "Enter your email"
                    },
                    {
                      "name": "subject",
                      "label": "Subject",
                      "type": "text",
                      "placeholder": "Enter subject"
                    },
                    {
                      "name": "message",
                      "label": "Message",
                      "type": "textarea",
                      "placeholder": "Enter your message"
                    }
                  ],
                  "items": {
                    "type": "object",
                    "fields": {
                      "name": {
                        "type": "short-text",
                        "label": "Field Name",
                        "required": true
                      },
                      "label": {
                        "type": "short-text",
                        "label": "Field Label",
                        "required": true
                      },
                      "type": {
                        "type": "select",
                        "label": "Field Type",
                        "options": ["text", "email", "tel", "textarea"],
                        "required": true
                      },
                      "placeholder": {
                        "type": "short-text",
                        "label": "Field Placeholder",
                        "required": true
                      }
                    }
                  }
                }
              }
            },
            "map_embed_url": {
              "type": "short-text",
              "label": "Map Embed URL",
              "format": "uri",
              "description": "Google Maps embed URL for your location",
              "required": true
            }
          }
        }
      }
    },
    "terms-of-service": {
      "type": "page",
      "title": "Terms of Service",
      "sections": {
        "main": {
          "title": "Terms Content",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Page Title",
              "minLength": 1,
              "required": true,
              "default": "Terms of Service"
            },
            "last_updated": {
              "type": "short-text",
              "label": "Last Updated Date",
              "pattern": "^\\d{4}-\\d{2}-\\d{2}$",
              "required": true,
              "default": "2024-01-01"
            },
            "related_documents": {
              "type": "array",
              "label": "Related Documents",
              "items": {
                "type": "object",
                "fields": {
                  "name": {
                    "type": "short-text",
                    "label": "Document Name",
                    "required": true,
                    "default": "Privacy Policy"
                  },
                  "url": {
                    "type": "short-text",
                    "label": "Document URL",
                    "required": true,
                    "default": "/privacy-policy"
                  },
                  "icon": {
                    "type": "short-text",
                    "label": "Document Icon Class",
                    "required": true,
                    "default": "fas fa-file-alt"
                  }
                }
              }
            },
            "contact_info": {
              "type": "object",
              "label": "Contact Information",
              "fields": {
                "email": {
                  "type": "short-text",
                  "label": "Contact Email",
                  "format": "email",
                  "required": true
                },
                "phone": {
                  "type": "short-text",
                  "label": "Contact Phone",
                  "required": true
                },
                "address": {
                  "type": "short-text",
                  "label": "Contact Address",
                  "required": true
                }
              }
            },
            "sections": {
              "type": "array",
              "label": "Terms Sections",
              "items": {
                "type": "object",
                "fields": {
                  "title": {
                    "type": "short-text",
                    "label": "Section Title",
                    "required": true,
                    "default": "General Terms"
                  },
                  "content": {
                    "type": "long-text",
                    "label": "Section Content",
                    "default": "These terms and conditions outline the rules and regulations for the use of our school's website."
                  },
                  "items": {
                    "type": "array",
                    "label": "Section Items",
                    "items": {
                      "type": "long-text",
                      "default": "By accessing this website, you accept these terms and conditions in full."
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "privacy-policy": {
      "type": "page",
      "title": "Privacy Policy",
      "sections": {
        "main": {
          "title": "Privacy Content",
          "fields": {
            "title": {
              "type": "short-text",
              "label": "Page Title",
              "minLength": 1,
              "required": true,
              "default": "Privacy Policy"
            },
            "last_updated": {
              "type": "short-text",
              "label": "Last Updated Date",
              "pattern": "^\\d{4}-\\d{2}-\\d{2}$",
              "required": true,
              "default": "2024-01-01"
            },
            "sections": {
              "type": "object",
              "label": "Privacy Policy Sections",
              "fields": {
                "information_collected": {
                  "type": "object",
                  "label": "Information We Collect",
                  "fields": {
                    "title": {
                      "type": "short-text",
                      "label": "Section Title",
                      "required": true,
                      "default": "Information We Collect"
                    },
                    "personal_info": {
                      "type": "object",
                      "fields": {
                        "title": {
                          "type": "short-text",
                          "label": "Personal Information Title",
                          "required": true,
                          "default": "Personal Information"
                        },
                        "items": {
                          "type": "array",
                          "label": "Personal Information Items",
                          "items": {
                            "type": "long-text",
                            "default": "Name, email address, and contact information provided through forms"
                          }
                        }
                      }
                    },
                    "technical_info": {
                      "type": "object",
                      "fields": {
                        "title": {
                          "type": "short-text",
                          "label": "Technical Information Title",
                          "required": true
                        },
                        "items": {
                          "type": "array",
                          "label": "Technical Information Items",
                          "items": {
                            "type": "long-text"
                          }
                        }
                      }
                    }
                  }
                },
                "information_usage": {
                  "type": "object",
                  "label": "How We Use Information",
                  "fields": {
                    "title": {
                      "type": "short-text",
                      "label": "Section Title",
                      "required": true
                    },
                    "items": {
                      "type": "array",
                      "label": "Usage Items",
                      "items": {
                        "type": "long-text"
                      }
                    }
                  }
                },
                "data_protection": {
                  "type": "object",
                  "label": "Data Protection",
                  "fields": {
                    "title": {
                      "type": "short-text",
                      "label": "Section Title",
                      "required": true
                    },
                    "items": {
                      "type": "array",
                      "label": "Protection Measures",
                      "items": {
                        "type": "long-text"
                      }
                    }
                  }
                },
                "information_sharing": {
                  "type": "object",
                  "label": "Information Sharing",
                  "fields": {
                    "title": {
                      "type": "short-text",
                      "label": "Section Title",
                      "required": true
                    },
                    "items": {
                      "type": "array",
                      "label": "Sharing Policies",
                      "items": {
                        "type": "long-text"
                      }
                    }
                  }
                },
                "user_rights": {
                  "type": "object",
                  "label": "User Rights",
                  "fields": {
                    "title": {
                      "type": "short-text",
                      "label": "Section Title",
                      "required": true
                    },
                    "items": {
                      "type": "array",
                      "label": "User Rights Items",
                      "items": {
                        "type": "long-text"
                      }
                    }
                  }
                }
              }
            },
            "quick_links": {
              "type": "array",
              "label": "Quick Links",
              "items": {
                "type": "object",
                "fields": {
                  "name": {
                    "type": "short-text",
                    "label": "Link Name",
                    "required": true
                  },
                  "url": {
                    "type": "short-text",
                    "label": "Link URL",
                    "required": true
                  }
                }
              }
            },
            "contact_info": {
              "type": "object",
              "label": "Contact Information",
              "fields": {
                "email": {
                  "type": "short-text",
                  "label": "Contact Email",
                  "format": "email",
                  "required": true,
                  "default": "privacy@school.edu"
                },
                "phone": {
                  "type": "short-text",
                  "label": "Contact Phone",
                  "required": true,
                  "default": "+1 (555) 123-4567"
                },
                "address": {
                  "type": "short-text",
                  "label": "Contact Address",
                  "required": true,
                  "default": "123 School Street, Education City, ST 12345"
                }
              }
            }
          }
        }
      }
    },
    "mandatory-disclosure": {
      "type": "page",
      "title": "Mandatory Disclosure",
      "sections": {
        "main": {
          "title": "Disclosure Content",
          "fields": {
            "last_updated": {
              "type": "short-text",
              "label": "Last Updated Date",
              "pattern": "^\\d{4}-\\d{2}-\\d{2}$",
              "default": "2024-01-01"
            },
            "school_info": {
              "type": "object",
              "label": "School Information",
              "fields": {
                "school_name": {
                  "type": "short-text",
                  "label": "School Name",
                  "minLength": 1,
                  "default": "Example School"
                },
                "affiliation_number": {
                  "type": "short-text",
                  "label": "Affiliation Number",
                  "minLength": 1,
                  "default": "ABC12345"
                },
                "school_code": {
                  "type": "short-text",
                  "label": "School Code",
                  "minLength": 1,
                  "default": "12345"
                },
                "address": {
                  "type": "long-text",
                  "label": "Address",
                  "minLength": 1,
                  "default": "123 School Street, Education City, ST 12345"
                },
                "principal_name": {
                  "type": "short-text",
                  "label": "Principal Name",
                  "minLength": 1,
                  "default": "Dr. John Smith"
                },
                "contact_details": {
                  "type": "long-text",
                  "label": "Contact Details",
                  "minLength": 1,
                  "default": "Phone: +1 (555) 123-4567\nEmail: contact@school.edu"
                }
              }
            },
            "infrastructure": {
              "type": "object",
              "label": "Infrastructure Details",
              "fields": {
                "campus_overview": {
                  "type": "array",
                  "label": "Campus Overview",
                  "items": {
                    "type": "object",
                    "fields": {
                      "name": {
                        "type": "short-text",
                        "label": "Feature Name",
                        "minLength": 1,
                        "default": "Campus Area"
                      },
                      "value": {
                        "type": "short-text",
                        "label": "Feature Value",
                        "minLength": 1,
                        "default": "5 acres"
                      },
                      "icon": {
                        "type": "short-text",
                        "label": "Feature Icon Class",
                        "minLength": 1,
                        "default": "fas fa-building"
                      }
                    }
                  }
                },
                "facilities": {
                  "type": "array",
                  "label": "Academic & Support Facilities",
                  "items": {
                    "type": "object",
                    "fields": {
                      "name": {
                        "type": "short-text",
                        "label": "Facility Name",
                        "minLength": 1
                      },
                      "details": {
                        "type": "long-text",
                        "label": "Facility Details",
                        "minLength": 1
                      },
                      "icon": {
                        "type": "short-text",
                        "label": "Facility Icon Class",
                        "minLength": 1
                      }
                    }
                  }
                },
                "additional_features": {
                  "type": "array",
                  "label": "Additional Features",
                  "items": {
                    "type": "object",
                    "fields": {
                      "name": {
                        "type": "short-text",
                        "label": "Feature Name",
                        "minLength": 1
                      },
                      "details": {
                        "type": "long-text",
                        "label": "Feature Details",
                        "minLength": 1
                      },
                      "icon": {
                        "type": "short-text",
                        "label": "Feature Icon Class",
                        "minLength": 1
                      }
                    }
                  }
                }
              }
            },
            "faculty_info": {
              "type": "array",
              "label": "Faculty Information",
              "default": [
                {
                  "department": "Primary Section",
                  "total_teachers": "25",
                  "qualification": "B.Ed, M.Ed",
                  "experience_range": "5-15 years"
                }
              ],
              "items": {
                "type": "object",
                "fields": {
                  "department": {
                    "type": "short-text",
                    "label": "Department",
                    "minLength": 1
                  },
                  "total_teachers": {
                    "type": "short-text",
                    "label": "Total Teachers",
                    "minLength": 1
                  },
                  "qualification": {
                    "type": "short-text",
                    "label": "Qualification",
                    "minLength": 1
                  },
                  "experience_range": {
                    "type": "short-text",
                    "label": "Experience Range",
                    "minLength": 1
                  }
                }
              }
            },
            "fee_structure": {
              "type": "array",
              "label": "Fee Structure",
              "items": {
                "type": "object",
                "fields": {
                  "class": {
